{
    "host": "localhost",
    "port": 5000,
    "paginate": {
    },
    "authentication": {
        "secret": "your secret",
        "strategies": [
            "jwt",
            "local"
        ],
        "path": "/authentication",
        "service": "users",
        "jwt": {
            "header": {
                "typ": "access"
            },
            "audience": "https://your-domain",
            "subject": "anonymous",
            "issuer": "feathers",
            "algorithm": "HS256",
            "expiresIn": "1d"
        },
        "local": {
            "entity": "user",
            "usernameField": "email",
            "passwordField": "password"
        }
    }
}
